    "def longest(s1, s2):\n",
    "    letters = [i for i in s1+s2]\n",
    "    letters.sort()\n",
    "    letters = list(dict.fromkeys(letters))\n",
    "    \n",
    "    string = \"\"\n",
    "    \n",
    "    for i in letters:\n",
    "        string += i\n",
    "        \n",
    "    return string"
   ]
