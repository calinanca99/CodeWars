    "def spin_words(sentence):\n",
    "    words = sentence.split(\" \")\n",
    "    for i in range(len(words)):\n",
    "        if len(words[i]) >= 5 and words[i]:\n",
    "            words[i] = words[i][::-1]\n",
    "\n",
    "    return \" \".join([word for word in words])"
   ]
